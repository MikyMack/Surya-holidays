    <style>
        .package-item {
            transition: all 0.3s ease;
        }

        .category-btn {
            transition: all 0.2s ease;
        }

        /* Add these styles to your existing CSS */
        .has-submenu {
            position: relative;
        }

        .submenu {
            display: none;
            position: absolute;
            min-width: 200px;
            background: white;
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
            z-index: 1000;
        }

        .has-submenu:hover .submenu {
            display: block;
        }

        .sub-submenu {
            display: none;
            position: absolute;
            left: 100%;
            top: 0;
            min-width: 200px;
            background: white;
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
        }

        .parent-menu-item:hover .sub-submenu {
            display: block;
        }

        .menu-arrow,
        .submenu-arrow {
            display: inline-block;
            margin-left: 5px;
            transition: transform 0.3s;
        }

        .has-submenu:hover .menu-arrow,
        .parent-menu-item:hover .submenu-arrow {
            transform: rotate(180deg);
        }

        /* Hide dropdowns on mobile */
        @media (max-width: 767px) {

            .menu-arrow,
            .submenu-arrow,
            .submenu,
            .sub-submenu {
                display: none !important;
            }

            #packageLink {
                pointer-events: auto !important;
            }
        }

        /* Animation */
        @keyframes fadeIn {
            from {
                opacity: 0;
            }

            to {
                opacity: 1;
            }
        }

        .animate-fade-in {
            animation: fadeIn 0.3s ease-out;
        }
    </style>
</head>

