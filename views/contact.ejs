                                <script>
                                    document.getElementById('myForm').addEventListener('submit', async function(e) {
    e.preventDefault();
    
    const form = e.target;
    const submitButton = form.querySelector('button[type="submit"]');
    const errorMsg = document.getElementById('error-msg');
    const simpleMsg = document.getElementById('simple-msg');
    
    // Show loading state
    submitButton.disabled = true;
    submitButton.innerHTML = 'Sending...';
    errorMsg.textContent = '';
    simpleMsg.textContent = '';
    simpleMsg.className = '';
    
    try {
        // Prepare form data
        const formData = new URLSearchParams(new FormData(form));
        
        // Replace with your deployed web app URL
        const scriptUrl = 'https://script.google.com/macros/s/AKfycbwgKz4lsiqwdo19W0sQ3x9Pdjxp7Iyn09SkJ9e-AJQyz4C1KU8F7F5amDFiQpibCRMoYA/exec';
        
        const response = await fetch(scriptUrl, {
            method: 'POST',
            body: formData,
            redirect: 'follow'
        });
        
        if (!response.ok) {
            throw new Error('Network response was not ok');
        }
        
        const result = await response.json();
        
        if (result.status === 'success') {
            // Show success message
            simpleMsg.textContent = 'Thank you! Your message has been sent.';
            simpleMsg.className = 'text-green-600 mt-2';
            form.reset();
        } else {
            throw new Error(result.message || 'Submission failed');
        }
    } catch (error) {
        console.error('Error:', error);
        errorMsg.textContent = `Error: ${error.message}`;
        errorMsg.className = 'text-red-600';
    } finally {
        submitButton.disabled = false;
        submitButton.textContent = 'Send Message';
    }
});
